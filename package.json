{
  "name": "customer-survey",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "engines": {
    "node": "^16.14.0"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "NODE_OPTIONS=\"--max_old_space_size=4096\" nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint:check": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:integration": "yarn reset:db && jest --runInBand --config ./test/integration/jest.json",
    "db": "docker-compose up -d",
    "seed:db": "npx prisma db seed",
    "clear:db": "ts-node ./test/configDb.ts",
    "reset:db": "yarn clear:db && yarn seed:db",
    "ci:local": "yarn test && yarn test:integration",
    "local": "yarn install && yarn db && yarn prisma migrate deploy && yarn seed:db && yarn test && yarn test:integration",
    "prisma:generate": "prisma generate --schema=./src/config/prisma/schema.prisma",
    "prisma:migrate": "prisma migrate dev --schema=./src/config/prisma/schema.prisma",
    "prisma:deploy": "npx prisma migrate deploy --schema=./src/config/prisma/schema.prisma",
    "prisma:pull": "npx prisma db pull --schema=./src/config/prisma/schema.prisma",
    "postinstall": "yarn prisma:generate"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@nestjs/axios": "^2.0.0",
    "@nestjs/common": "^9.0.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/jwt": "^10.0.3",
    "@nestjs/mapped-types": "*",
    "@nestjs/passport": "^10.0.1",
    "@nestjs/platform-express": "^9.4.3",
    "@nestjs/terminus": "^9.2.2",
    "@prisma/client": "^5.6.0",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "prisma": "^5.1.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "twilio": "^4.11.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/swagger": "^7.1.8",
    "@nestjs/testing": "^9.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.13",
    "@types/jest": "28.1.8",
    "@types/node": "^16.0.0",
    "@types/passport-jwt": "^3.0.8",
    "@types/passport-local": "^1.0.35",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "28.1.3",
    "joi": "^17.9.2",
    "nock": "^13.3.3",
    "passport-jwt": "^4.0.1",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.8",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.0",
    "typescript": "~4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
